@model IEnumerable<Teledocer.Models.Clients>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Inn_kl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name_corp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data_add)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data_reload)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Inn_kl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name_corp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_add)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_reload)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_clients">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_clients">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_clients">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
